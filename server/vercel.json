{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -e ."
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "OPEN_AI": "@open_ai_key"
  },
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
}
